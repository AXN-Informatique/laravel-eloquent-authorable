{
    "name": "axn/laravel-eloquent-authorable",
    "description": "Support for created_by and updated_by fields in Eloquent models.",
    "homepage": "https://github.com/AXN-Informatique/laravel-eloquent-authorable",
    "support": {
        "issues": "https://github.com/AXN-Informatique/laravel-eloquent-authorable/issues"
    },
    "authors": [
        {
            "name": "AXN Informatique",
            "email": "developpement@axn.fr"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/framework" : "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.21.2",
        "driftingly/rector-laravel": "^2.0.2",
        "rector/rector": "^2.0.10"
    },
    "autoload": {
        "psr-4": {
            "Axn\\EloquentAuthorable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axn\\EloquentAuthorable\\ServiceProvider"
            ]
        }
    }
}
